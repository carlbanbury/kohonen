{
  "name": "kohonen",
  "version": "0.7.0",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "description": "A basic implementation of a Kohonen map in JavaScript",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-core/register --recursive",
    "test:local": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive -w",
    "build": "./node_modules/.bin/babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seracio/kohonen.git"
  },
  "author": "nmondon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seracio/kohonen/issues"
  },
  "homepage": "https://github.com/seracio/kohonen#readme",
  "dependencies": {
    "d3-array": "1.0.2",
    "d3-random": "1.0.2",
    "d3-scale": "1.0.4",
    "filendir": "^1.0.0",
    "lodash": "4.17.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "3.5.0",
    "chai-spies": "0.7.1",
    "docco": "0.7.0",
    "eslint": "3.11.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.2.0"
  }
